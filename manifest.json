{
  "manifest_version": 3,
  "name": "Steam Market Stats",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "storage",
    "webRequest",
    "https://steamcommunity.com/market/itemordershistogram*"
  ],
  "host_permissions": [
    "https://steamcommunity.com/market/itemordershistogram*"
  ]
}
